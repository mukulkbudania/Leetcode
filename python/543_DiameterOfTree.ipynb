    "class Solution(object):\n",
    "    def diameterOfBinaryTree(self, root):\n",
    "        if root is None:\n",
    "            return 0\n",
    "        lh, ld = self.diameterOfBinaryTree(root.left)\n",
    "        rh, rd = self.diameterOfBinaryTree(root.right)\n",
    "        return 1+max(lh,rh), max (1 + lh + rh, ld, rd) "
   ]
